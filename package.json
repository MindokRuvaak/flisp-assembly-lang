{
  "name": "flisp-assembly-lang",
  "displayName": "Flisp Assembly Lang",
  "description": "Syntax highlighting for FLISP Assembly language",
  "version": "0.0.2",
  "repository": "https://github.com/MindokRuvaak/flisp-assembly-lang",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "FLISPAssembly",
      "aliases": ["FLISPAssembly", "FLISP Assembly", "flispass"],
      "extensions": [".sflisp"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "FLISPAssembly",
      "scopeName": "source.FLISPAssembly",
      "path": "./syntaxes/FLISPAssembly.tmLanguage.json"
    }]
  }
}
