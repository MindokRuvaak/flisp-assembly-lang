{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "FLISP Assembly",
	"patterns": [
		{
			"include": "#symbol"
		},
		{
			"include": "#mnemonic"
		},
		{
			"include": "#operand"
		}
	],
	"repository": {
		"keywords": {
			"patterns": [{
				"name": "keyword.control.flispass",
				"match": "\\b(if|while|for|return)\\b"
			}]
		},
		"strings": {
			"name": "string.quoted.double.flispass",
			"begin": "\"",
			"end": "\"",
			"patterns": [
				{
					"name": "constant.character.escape.flispass",
					"match": "\\\\."
				}
			]
		}
	},
	"scopeName": "source.flispassembly"
}